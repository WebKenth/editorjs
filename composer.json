{
    "name": "webkenth/editorjs",
    "description": "Next generation block styled editor.",
    "keywords": [
        "msuhels",
        "laravel",
        "editorjs"
    ],
    "authors": [
        {
            "name": "Mohammad Suhel Shaikh",
            "email": "m.suhel11@gmail.com",
            "role": "Creator / Founder"
        },
        {
            "name": "Martin Knudsen",
            "email": "mkk@webkenth.dk",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {},
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "msuhels\\editorjs\\": "src",
            "msuhels\\editorjs\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "msuhels\\editorjs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "msuhels\\editorjs\\FilamentEditorJsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
